{
  "installCommand": "npm install",
  "buildCommand": "npm run build -- --configuration=production",
  "outputDirectory": "dist/gerenciador-de-alunos",
  "framework": "angular",
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
